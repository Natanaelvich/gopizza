{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"],
    },
    "plugins": [
        {
          "name": "typescript-styled-plugin",
          "lint": {
            "validProperties": [
              "elevation"
            ]
          }
        }
      ]
  },
  "extends": "expo/tsconfig.base"
}
